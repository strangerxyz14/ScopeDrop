# Supabase Configuration
VITE_SUPABASE_URL=https://your-project-ref.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# API Keys
VITE_GNEWS_API_KEY=your-gnews-api-key-here
VITE_GEMINI_API_KEY=your-gemini-api-key-here

# Environment Configuration
NODE_ENV=development
VITE_APP_ENV=staging

# Cache Configuration
VITE_CACHE_TTL_BROWSER=1800000
VITE_CACHE_TTL_DATABASE=21600000
VITE_CACHE_MAX_SIZE=10485760

# API Rate Limits (Staging)
VITE_GNEWS_DAILY_LIMIT=100
VITE_GNEWS_HOURLY_LIMIT=10
VITE_GEMINI_DAILY_LIMIT=1500
VITE_GEMINI_HOURLY_LIMIT=150

# Content Refresh Intervals (in milliseconds)
VITE_REFRESH_INTERVAL_HIGH=7200000    # 2 hours
VITE_REFRESH_INTERVAL_MEDIUM=14400000 # 4 hours
VITE_REFRESH_INTERVAL_LOW=43200000    # 12 hours

# Edge Function Configuration
VITE_USE_EDGE_FUNCTIONS=true
VITE_EDGE_FUNCTION_URL=https://your-project-ref.supabase.co/functions/v1

# Performance Monitoring
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_PERFORMANCE_SAMPLE_RATE=0.1

# Analytics
VITE_ENABLE_ANALYTICS=true
VITE_ANALYTICS_SAMPLE_RATE=0.5

# Development Settings
VITE_ENABLE_DEBUG_LOGGING=true
VITE_ENABLE_CACHE_DEBUG=false
VITE_ENABLE_API_DEBUG=false

# Production Overrides (uncomment for production)
# NODE_ENV=production
# VITE_APP_ENV=production
# VITE_GNEWS_DAILY_LIMIT=1000
# VITE_GNEWS_HOURLY_LIMIT=100
# VITE_GEMINI_DAILY_LIMIT=15000
# VITE_GEMINI_HOURLY_LIMIT=1500
# VITE_ENABLE_DEBUG_LOGGING=false